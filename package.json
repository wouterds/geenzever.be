{
    "name": "geenzever",
    "version": "1.0.3",
    "private": true,
    "scripts": {
        "lint": "tslint --format stylish './src/**/*.ts' './src/**/*.tsx' && stylelint './src/**/*.ts'",
        "lint:fix": "tslint --format stylish --fix './src/**/*.ts' './src/**/*.tsx' && stylelint --fix './src/**/*.ts'",
        "build": "NODE_ENV=production next build ./src && NODE_ENV=production tsc -p ./tsconfig.server.json",
        "sync:models": "ts-node -r ./src/bootstrap ./src/scripts/sync-models",
        "notify:requested-stickers": "node -r ./src/bootstrap ./src/scripts/notify-requested-stickers",
        "notify:dispatched-stickers": "node -r ./src/bootstrap ./src/scripts/notify-dispatched-stickers",
        "dev": "next ./src",
        "server": "NODE_ENV=production node -r ./src/bootstrap ./src/server",
        "version:patch": "json-bump package.json --patch && npm run version:_",
        "version:minor": "json-bump package.json --minor && npm run version:_",
        "version:major": "json-bump package.json --major && npm run version:_",
        "version:_": "npm i && git add package*.json -p && git commit -m \":bookmark: $(node -p \"require('./package.json').version\")\" && git tag $(node -p \"require('./package.json').version\")"
    },
    "dependencies": {
        "@sentry/browser": "^5.1.1",
        "@sentry/node": "^5.1.0",
        "axios": "^0.18.0",
        "body-parser": "^1.19.0",
        "cors": "^2.8.5",
        "date-fns": "^1.30.1",
        "dotenv": "^7.0.0",
        "express": "^4.16.4",
        "http-status": "^1.3.2",
        "i18next": "^15.1.0",
        "mariadb": "^2.0.3",
        "next": "^8.1.1-canary.16",
        "next-ga": "^2.3.4",
        "nprogress": "^0.2.0",
        "qs": "^6.7.0",
        "react": "^16.8.6",
        "react-dom": "^16.8.6",
        "react-i18next": "^10.9.0",
        "redis": "^2.8.0",
        "sequelize": "^5.7.6",
        "sparkpost": "^2.1.3",
        "spectre.css": "^0.5.8",
        "styled-bootstrap-grid": "^3.0.2",
        "styled-components": "^4.2.0",
        "styled-components-breakpoint": "^2.1.1",
        "tsconfig-paths": "^3.8.0",
        "validator": "^10.11.0"
    },
    "devDependencies": {
        "@types/body-parser": "^1.17.0",
        "@types/cors": "^2.8.5",
        "@types/dotenv": "^6.1.1",
        "@types/express": "^4.16.1",
        "@types/next": "^8.0.4",
        "@types/node": "^11.13.8",
        "@types/nprogress": "0.0.29",
        "@types/qs": "^6.5.3",
        "@types/react": "^16.8.14",
        "@types/react-dom": "^16.8.4",
        "@types/react-i18next": "^8.1.0",
        "@types/redis": "^2.8.12",
        "@types/sparkpost": "^2.1.4",
        "@types/styled-components": "^4.1.14",
        "@types/validator": "^10.11.0",
        "@zeit/next-bundle-analyzer": "^0.1.2",
        "@zeit/next-css": "^1.0.1",
        "@zeit/next-typescript": "^1.1.1",
        "babel-plugin-module-resolver": "^3.2.0",
        "dotenv-webpack": "^1.7.0",
        "imagemin-gifsicle": "^6.0.1",
        "imagemin-mozjpeg": "^8.0.0",
        "imagemin-optipng": "^6.0.0",
        "imagemin-svgo": "^7.0.0",
        "json-bump": "^0.1.3",
        "next-compose-plugins": "^2.1.1",
        "next-fonts": "^0.17.0",
        "next-optimized-images": "^2.5.0",
        "prettier": "^1.17.0",
        "stylelint": "^10.0.1",
        "stylelint-config-recommended": "^2.2.0",
        "stylelint-config-styled-components": "^0.1.1",
        "stylelint-processor-styled-components": "^1.6.0",
        "ts-node": "^8.1.0",
        "tslint": "^5.16.0",
        "tslint-config-prettier": "^1.18.0",
        "tslint-config-standard": "^8.0.1",
        "tslint-plugin-prettier": "^2.0.1",
        "typescript": "^3.4.5"
    }
}
